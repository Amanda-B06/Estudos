programa {

  caracter empresa
  inteiro codigo

  funcao inicio() {
    //Letra e codigo da empresa
   escreva ("Letra do setor (F, V, M, etc.): ")
   leia (empresa)

   escreva ("Código do funcionário: ")
   leia (codigo)

    //Saída
    escreva ("Funcionário do setor ", empresa , ", código ", codigo)

   
  }
}
