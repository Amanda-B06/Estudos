programa {

   real A, B, C, Delta

  funcao inicio() {
     //Pergunta A
      escreva ("Digite o valor de A: ")
      leia (A)

     //Pergunta B
      escreva ("Digite o valor de B também: ")
      leia (B)

     //Pergunta B
      escreva ("E a do C: ")
      leia (C)

     //Conta
      Delta= (B*B)-4*A*C
      
     //Saída
      escreva ("O valor de Delta é: ", Delta)
 
  }
}
