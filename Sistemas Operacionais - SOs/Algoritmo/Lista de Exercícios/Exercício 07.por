programa {
   real Num1, Dobro, Terca

  funcao inicio() {
    //Pergunta para o número
    escreva ("Digite um número: ")
    leia (Num1)
     
    //O dobro
    Dobro=Num1*2
    escreva ("O dobro de ", Num1, " é: ", Dobro, "\n")
 
    //A terça parte
    Terca=Num1/3
    escreva ("A terça parte de ", Num1, " é: ", Terca, ".")
  }
}
